<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home"
    app:startDestination="@id/home_fragment">

    <fragment
        android:id="@+id/home_fragment"
        android:name="com.agro.inventory.ui.home.HomeFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_home" >


        <action
            android:id="@+id/action_home_fragment_to_invent_assigment_fragment"
            app:destination="@id/invent_assigment_fragment" />
    </fragment>

    <fragment
        android:id="@+id/invent_assigment_fragment"
        android:name="com.agro.inventory.ui.invent.InventAssigmentFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_invent_assigment" >


        <action
            android:id="@+id/action_home_fragment_to_kode_plot_fragment"
            app:destination="@id/kode_plot_fragment" />
        <action
            android:id="@+id/action_invent_assigment_fragment_to_home_fragment"
            app:destination="@id/home_fragment" />

    </fragment>

    <fragment
        android:id="@+id/kode_plot_fragment"
        android:name="com.agro.inventory.ui.invent.PlotFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_plot" >

        <argument
            android:name="areaId"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="titleArea"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="kode"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="nameMember"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="plotCount"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />


        <action
            android:id="@+id/action_kode_plot_fragment_to_comodity_fragment"
            app:destination="@id/comodity_fragment" />
        <action
            android:id="@+id/action_kode_plot_fragment_to_invent_assigment_fragment"
            app:destination="@id/invent_assigment_fragment" />
    </fragment>

    <fragment
        android:id="@+id/comodity_fragment"
        android:name="com.agro.inventory.ui.invent.ComodityFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_comodity" >

        <argument
            android:name="areaId"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="titleArea"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="kode"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="nameMember"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="plotCount"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />


        <action
            android:id="@+id/action_kode_plot_fragment_to_monitoring_worker_fragment"
            app:destination="@id/monitoring_worker_fragment" />
    </fragment>

    <fragment
        android:id="@+id/monitoring_worker_fragment"
        android:name="com.agro.inventory.ui.invent.InventFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_invent" >

        <argument
            android:name="idPlot"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="kodePlot"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="polaTanam"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="komoditas"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="areaName"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="memberNo"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />


    </fragment>


    <fragment
        android:id="@+id/edit_monitoring_worker_fragment"
        android:name="com.agro.inventory.ui.invent.editinvent.EditInventFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_edit_invent" >

        <argument
            android:name="id"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="kodePlot"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="polaTanam"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="komoditas"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="namaPekerja"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="pekerjaan"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="activities"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="volume"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="satuan"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="lat"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="lng"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

        <argument
            android:name="photo"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />

    </fragment>


</navigation>